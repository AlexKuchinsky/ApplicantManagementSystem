@model IEnumerable<EntrantsManagementSystem.Models.DeleteLog>
@using EntrantsManagementSystem.Models
@{
    ViewBag.Title = "DeleteLogsList";
}

<h2>Delete Logs</h2>

<table class="table table-bordered table-dark">
    <tr>
        <td>
            <b>Log ID</b>
        </td>
        <td>
            <b>Time</b>
        </td>
        <td>
            <b>Operation</b>
        </td>
        <td>
            <b>Object</b>
        </td>
    </tr>
    @foreach (DeleteLog deleteLog in Model)
    {
        <tr>
            <td>
                @deleteLog.Log.LogID
            </td>
            <td>
                @deleteLog.Log.StartTime
            </td>
            <td>
                @deleteLog.Log.Operation
            </td>
            <td>
                @deleteLog.ObjectName
            </td>
       
        </tr>
    }
</table>
